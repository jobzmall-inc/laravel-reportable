{
    "name": "jobzmall/reportable",
    "description": "Reportable Polymorphic Eloquent Models for Laravel 8",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-reportable",
        "Laravel Reportable"
    ],
    "license": "MPL-2.0",
    "authors": [{
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        },
        {
            "name": "Logan Lehman",
            "email": "logan@jobzmall.com",
            "homepage": "https://jobzmall.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Reportable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Reportable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Reportable\\ReportableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
